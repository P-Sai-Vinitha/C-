#include <iostream>
using namespace std;

int main() {
	int t;
	cin>>t;
	while(t--)
	{
	    int a,c=0,m=-10;
	    cin>>a;
	    int arr[a],brr[a];
	    for(int i=0;i<a;i++)
	    {
	        cin>>arr[i];
	    }
	    for(int i=0;i<a;i++)
	    {
	        cin>>brr[i];
	    }
	    for(int i=0;i<a;i++)
	    {
	        if(arr[i]!=0&&brr[i]!=0)
	        {
	            c++;
	        }
	        else
	        {
	            c=0;
	        }
	        m=max(m,c);
	    }
	    cout<<m<<endl;
	    
	}
	return 0;
}
